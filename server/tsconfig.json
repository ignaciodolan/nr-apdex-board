    "outDir": "./dist",
    "baseUrl": "./lib",
    "paths": {
      "*": [
        "node_modules/*"
      ]
    }
  },
  "include": [
    "**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "data"
  ]
}